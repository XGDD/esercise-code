public class Test{
    public static void main(String[] args){
        String str = "165788";
        System.out.println(isNumber(str));
    }
    public static boolean isNumber(String str){
        char[] str2 = str.toCharArray();
        for(int i=0; i<str2.length; i++){
            if(str2[i] < '0' || str2[i] > '9'){
                return false;
            }
        }
        return true;
    }
}
