void reverse(int arr[5],int n)
{
	int i, j, t;
	for (i = 0, j = n - 1; i < j; i++, j--)
	{
		t = arr[i];
		arr[i] = arr[j];
		arr[j] = t;
	}
	for (i = 0; i < n;i++)
	{
		printf("%d ", arr[i]);
	}
	printf("\n");

}

int main()
{
	int n = 5;
	int arr[5] = { 1, 2, 3, 4, 5 };
	empty(arr,n);
	reverse(arr,n);

	return 0;
}
