#define _CRT_SECURE_NO_WARNINGS 1

#include<stdio.h>

int fib1(int n)
{
	int a = 1;
	int b = 1;
	int c = a;
	while (n > 2)
	{
		c = a + b;
		a = b;
		b = c;
		n--;
	}
	return c;
}

int fib2(int n)
{
	if (n <= 2)
	{
		return 1;
	}
	else
	{
		return fib2(n - 1) + fib2(n - 2);
	}
}

int main()
{
	int n = 0;
	printf("please input a numberï¼›\n");
	scanf("%d", &n);
	int ret = fib1(n);
	printf("%d\n",ret);

	return 0;
}
